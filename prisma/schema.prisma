generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model usuario {
  id               Int     @id @default(autoincrement())
  nombre           String
  dni              BigInt  @unique
  cuil             String
  correo           String  @unique
  password         String
  telefono         BigInt
  fecha_nacimiento String
  dni_foto_delante String
  dni_foto_detras  String
  textextract      Json  
  verificacion_log Json
}

